public class Boat extends Vehicle {
	 public Boat(String name,int kilometers) {  
        super(name, kilometers);
	}
	
	//@Override  
    public String doStuff() {  
        return "Je suis " + this.getbrand() + " et je fais glou glou !";  
    }
}
public class Car extends Vehicle {
	
	public Car(String name,int kilometers) {  
        super(name, kilometers); 
    }
	
	//@Override
    public String doStuff() {  
        return "Je suis " + this.getbrand() + " et je fais vroum vroum !";  
    }

}
public class Hangar {
    public static void main(String[] args) {
    	Car Merco = new Car("cla",12555);
		Boat maritime = new Boat("speed",5545);
    	System.out.println(Merco.doStuff());
    	System.out.println(maritime.doStuff());
    }
}
public abstract class Vehicle {
    //-----------------------------------------------------
	//les atributs
    //-----------------------------------------------------
	private String brand; 
	private int kilometers;
	
	
	
    //-----------------------------------------------------
	//les m√©thodes
    //-----------------------------------------------------
	
	// constructeurs
	public Vehicle(String brand, int kilometers){
		this.brand = brand;
		this.kilometers = kilometers;
	}
	
	
	public Vehicle(String brand){
		this.brand = brand;
		this.kilometers = 100;
	}
	
	public Vehicle(int kilometers){
		this.brand = "amine";
		this.kilometers = kilometers;
	}
	
	
	public Vehicle(String brand){
		this.brand = brand;
	}
	
	public Vehicle(String kilometers){
		this.kilometers = kilometers;
	}
	
	// accesseurs et mutateurs 
	 
    public String getbrand() {  
        return this.brand;  
    }  

    public void setbrand(String brand) {  
        this.brand = brand;  
    }  

    public int getkilometers() {  
        return this.kilometers;  
    }  

    public void setkilometers(int kilometers) {
        this.kilometers = kilometers;
    }
	
	public abstract String doStuff();
}
